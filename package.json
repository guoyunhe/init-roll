{
  "name": "init-roll",
  "version": "1.3.1",
  "description": "Framework for project creation and migration",
  "keywords": [
    "node",
    "cli"
  ],
  "homepage": "https://github.com/guoyunhe/init-roll#readme",
  "bugs": {
    "url": "https://github.com/guoyunhe/init-roll/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guoyunhe/init-roll.git"
  },
  "funding": "https://github.com/sponsors/guoyunhe",
  "license": "MIT",
  "author": {
    "name": "Guo Yunhe",
    "email": "i@guoyunhe.me"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rive build",
    "build:watch": "rive build --watch",
    "ci:eslint": "eslint -f json src -o ./.ci/eslint.json",
    "lint": "eslint .",
    "lint:fix": "prettier --write . && eslint --fix .",
    "prepare": "husky",
    "start": "rive start",
    "test": "rive test",
    "test:ui": "rive test --ui",
    "test:watch": "rive test --watch"
  },
  "lint-staged": {
    "*.{cjs,cts,js,jsx,mjs,mts,ts,tsx,vue}": "eslint --fix",
    "*.{cjs,css,cts,html,js,json,jsx,less,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}": "prettier --write"
  },
  "prettier": "prettier-config-ali",
  "dependencies": {
    "@guoyunhe/sleep": "^1.0.0",
    "chalk": "^5.3.0",
    "deepmerge": "^4.3.1",
    "ejs": "^3.1.10",
    "fast-deep-equal": "^3.1.3",
    "fast-glob": "^3.3.2",
    "json5": "^2.2.3",
    "latest-version": "^9.0.0",
    "package-json-from-git": "^1.2.2",
    "prettier": "^3.2.5",
    "sort-package-json": "^2.10.0"
  },
  "devDependencies": {
    "@mdx-js/react": "^3.1.0",
    "@types/ejs": "^3.1.5",
    "@types/node": "^20.17.30",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "eslint-config-ali": "^16.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier-config-ali": "^1.3.2",
    "react": "^18.3.1",
    "react-doc-ui": "^2.3.0",
    "react-dom": "^18.3.1",
    "rive": "^3.0.1",
    "typescript": "^5.8.3"
  },
  "rive": {
    "template": "node",
    "doc": {
      "basename": "/init-roll/"
    }
  }
}
